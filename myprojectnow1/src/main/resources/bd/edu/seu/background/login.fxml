<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.background.LoginController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@photo_6262732105289156354_x.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="userTextfield" layoutX="360.0" layoutY="256.0" />
      <Button layoutX="619.0" layoutY="445.0" mnemonicParsing="false" onAction="#adminloginto" style="-fx-background-color: yellow;" text="ADMIN">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Button layoutX="324.0" layoutY="361.0" mnemonicParsing="false" onAction="#loginto" style="-fx-background-color: yellow;" text="Login">
